{
  "name": "wuestkamp/alterable-form-bundle",
  "description": "Allows Symfony Forms to be altered based on YAML configuration files",
  "keywords": ["symfony", "symfony2", "changeable", "forms", "alterable", "configurable"],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "symfony/form": "^2.8|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "symfony/validator": "^2.8|^3.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "symfony/security": "^2.8|^3.0",
    "mockery/mockery": "^0.9.9"
  },
  "autoload": {
    "psr-4": {
      "Wuestkamp\\AlterableFormBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wuestkamp\\AlterableFormBundle\\": "tests/"
    }
  }
}
